    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Debug server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "debug sequelize",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "sequelize",
            "runtimeArgs": [
                "db:seed:all"
            ],
        }
    ]
}